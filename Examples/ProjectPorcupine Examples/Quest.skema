~Version: 1~
~DocumentVersion: ""~

define Param:
{ 
    Name: String,
    Value: Any,
},

define Goal:
{
    Description: String,
    IsCompletedLuaFunction: String,
    Params: [ #Param ],
},

define Reward:
{
    Description: String,
    OnRewardLuaFunction: String,
    Params: [ #Param ],
},

define Quest:
{
    Name: String,

    Description: String,

    optional PreRequiredCompletedQuests: [ String ],

    Goals: [ #Goal ],

    Rewards: [ #Rewards ],
},

Quests: [ #Quest ],